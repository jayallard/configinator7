{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "environmentRules": {
    "environmentTypes": [
      {
        "supportsPreRelease": true,
        "environmentTypeName": "development",
        "next": "staging",
        "allowedEnvironments": [
          "development",
          "development-jay",
          "development-santa-claus"
        ]
      },
      {
        "supportsPreRelease": true,
        "environmentTypeName": "staging",
        "next": "production",
        "allowedEnvironments": [
          "staging"
        ]
      },
      {
        "environmentTypeName": "production",
        "allowedEnvironments": [
          "production"
        ]
      }
    ]
  }
}
